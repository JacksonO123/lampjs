{
  "name": "@jacksonotto/lampjs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "A lightweight front-end JavaScript framework",
  "author": "Jackson Otto",
  "version": "4.5.1",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx nodemon -e ts --watch src --exec npx tsc"
  },
  "exports": {
    ".": {
      "import": "./dist/lampjs.js",
      "types": "./dist/lampjs.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "latest",
    "nodemon": "latest",
    "typescript": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksonO123/lampjs.git"
  },
  "keywords": [
    "lampjs",
    "frontend",
    "javascript",
    "typescript",
    "framework",
    "jsx"
  ]
}
