{
  "name": "@jacksonotto/lampjs-ssr",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "description": "A server side rendering runtime for the LampJs framework",
  "author": "Jackson Otto",
  "version": "0.3.4",
  "bin": {
    "lampjs-ssr": "./dist/bin.js"
  },
  "scripts": {
    "watch": "npx nodemon -e ts,cts --watch src --exec npx tsc"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/server.d.ts"
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/node": "latest",
    "typescript": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JacksonO123/lampjs.git"
  },
  "keywords": [
    "lampjs",
    "frontend",
    "backend",
    "javascript",
    "typescript",
    "framework",
    "jsx",
    "ssr"
  ],
  "dependencies": {
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.8",
    "express": "^4.18.2",
    "mime-types": "^2.1.35",
    "nodemon": "^3.0.1",
    "vite": "^5.0.2"
  },
  "peerDependencies": {
    "@jacksonotto/lampjs": "^4.5.1"
  }
}
