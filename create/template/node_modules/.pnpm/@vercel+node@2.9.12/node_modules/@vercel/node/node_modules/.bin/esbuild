#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/bin/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/bin/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules/esbuild/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/esbuild@0.14.47/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
fi
"$basedir/../../../../../../esbuild@0.14.47/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
