#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules/ts-node/dist/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules/ts-node/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules/ts-node/dist/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules/ts-node/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/ts-node@10.9.1_jh3j7gf3qptnaxtpzk3sx5vjku/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
