#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/dist/cli/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/dist/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/dist/cli/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/dist/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules/edge-runtime/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/edge-runtime@2.0.0/node_modules:/Users/jacksonotto/code/lampjs/create/template/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../dist/cli/index.js" "$@"
fi
